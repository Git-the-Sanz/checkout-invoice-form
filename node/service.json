{
  "memory": 256,
  "ttl": 10,
  "timeout": 5000,
  "minReplicas": 2,
  "maxReplicas": 4,
  "routes": {
    "getSchema": {
      "path": "/invoice/getSchema/:schema",
      "public": true,
      "settingsType": "workspace"
    },
    "generateSchema": {
      "path": "/invoice/generateSchema",
      "public": true,
      "settingsType": "workspace"
    },
    "getDocuments": {
      "path": "/invoice/getDocuments/:schemaName/:type/:whereClause",
      "public": true,
      "settingsType": "workspace"
    },
    "saveDocuments": {
      "path": "/invoice/saveDocuments/:schemaName",
      "public": true,
      "settingsType": "workspace"
    }
  }
}
